{
  "name": "storybook-addon-vue-slots",
  "version": "0.9.29",
  "description": "Vue Slots support for Storybook",
  "type": "module",
  "keywords": [
    "storybook-addons",
    "storybook",
    "vue",
    "vue3",
    "slots"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JoJk0/storybook-addon-vue-slots"
  },
  "author": "Jacob Janisz",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "import": "./dist/preview.js",
      "types": "./dist/preview.d.ts",
      "require": "./dist/preview.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "yarn clean",
    "build": "tsup",
    "build:watch": "pnpm build --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch 'storybook --quiet'",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "yarn build && auto shipit",
    "storybook": "storybook dev -p 6066",
    "build-storybook": "storybook build -o storybook-static",
    "preview-storybook": "vite preview --port 6088 --out-dir storybook-static"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-mdx-gfm": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/builder-vite": "^8.1.9",
    "@storybook/components": "^8.1.9",
    "@storybook/core-events": "^8.1.9",
    "@storybook/preview-api": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/theming": "^8.1.9",
    "@storybook/types": "^8.1.9",
    "@storybook/vue3": "^8.1.9",
    "@storybook/vue3-vite": "^8.1.9",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "auto": "^11.1.6",
    "boxen": "^7.1.1",
    "cheerio": "1.0.0-rc.12",
    "dedent": "^1.5.3",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "storybook": "^8.1.9",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vue": "^3.4.29",
    "zx": "^8.1.2"
  },
  "peerDependencies": {
    "@storybook/blocks": "^8.1.9",
    "@storybook/core-events": "^8.1.9",
    "@storybook/preview-api": "^8.1.9",
    "@storybook/types": "^8.1.9",
    "@storybook/vue3": "^8.1.9",
    "vue": "^3.3.4"
  },
  "storybook": {
    "displayName": "Vue Slots",
    "supportedFrameworks": [
      "vue3"
    ],
    "icon": "https://api.iconify.design/logos/vue.svg"
  },
  "publishConfig": {
    "access": "public"
  },
  "auto": {
    "plugins": [
      "npm"
    ]
  }
}
